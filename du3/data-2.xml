    <TypyZvirat>
        <ZvireTyp nazev="Zebra">
            <nazev>Zebra</nazev>
            <popis xml:lang="cs">Bily kuň s Cernyma pruhama</popis>
            <popisChovu xml:lang="cs">Zebry maji rady slunne teple travnate plane, živi se travou, senem. Zimu travi ve stajich</popisChovu>
            <TypStravy nazevTypu="Seno"/>
        </ZvireTyp>
        
        <ZvireTyp nazev="VelbloudDvouhrby">
            <nazev>Velbloud dvouhrby</nazev>
            <popis xml:lang="cs">Hnedy kuň s dvouma hrbama</popis>
            <popisChovu xml:lang="cs">Velbloud je velmi nenaroCne zvire, živi se travou, senem. Zimu travi ve stajich. Když jednou zapomenete dodat vodu, nic se nestane.</popisChovu>
            <TypStravy nazevTypu="Seno"/>
        </ZvireTyp>

        <ZvireTyp nazev="PaovceHrivnata">
            <nazev>Paovce hrivnata</nazev>
            <popis xml:lang="cs">Jedna se o druh z podCeledi kozy a ovce puvodem ze skal hor v Severni Africe.</popis>
            <popisChovu xml:lang="cs">Živi se travou, senem. Zimu travi ve stajich.</popisChovu>
            <TypStravy nazevTypu="Seno"/>
        </ZvireTyp>

        <ZvireTyp nazev="KobraKralovska">
            <nazev>Kobra kralovska</nazev>
            <popis xml:lang="cs">je druhem jedovateho hada z Celedi koralovcovitych, ktery žije v oblasti jižni a jihovychodni Asie.</popis>
            <popisChovu xml:lang="cs">Živi se myšmi, nutne je byt opatrny, jedna se o jednovateho hada.</popisChovu>
            <TypStravy nazevTypu="Myš"/>
        </ZvireTyp>

        <ZvireTyp nazev="GorilaNižnna">
            <nazev>Gorila Nižnna</nazev>
            <popis xml:lang="cs">Domovem goril jsou tropicke lesy v rovnikove Africe, kde se vyskytuji ve dvou oblastech oddelenych deštnym lesem konžske panve.</popis>
            <popisChovu xml:lang="cs">Živi se Casti rostlin, ovoce, mravenci.</popisChovu>
            <TypStravy nazevTypu="Meloun"/>
